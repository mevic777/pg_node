<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Categories</title>
</head>
<body>
    <% include('./partials/nav.ejs'); %>
    <h1>Categories</h1>
    <% if (categories.length) { %>
        <% categories.forEach((category) => { %>
        <li>
            <a href="/categories/<%= category.id %>"><%= category.nume %></a>
        </li>
        <% }) %>
    <% } %>
</body>
</html>